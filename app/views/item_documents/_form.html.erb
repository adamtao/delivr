<%= simple_form_for(@item_document) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :item_id %>
    <%= f.input :document_file_name %>
    <%= f.input :document_content_type %>
    <%= f.input :document_file_size %>
    <%= f.input :document_updated_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
